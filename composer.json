{
    "name": "thewebmen/silverstripe-articles",
    "description": "Articles module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "articles",
        "blog",
        "news",
        "listing"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Michel van der Steege",
        "email": "michel@thewebmen.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^5",
        "silverstripe/vendor-plugin": "^2.0",
        "silverstripe/lumberjack": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "silverstripe-articles",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "TheWebmen\\Articles\\": "src/"
        }
    }
}
